        <ec-service-user.version>1.0.0</ec-service-user.version>
        <ec-service-goods-api.version>1.0.0</ec-service-goods-api.version>
        <ec-service-goods.version>1.0.0</ec-service-goods.version>
        <ec-service-order-api.version>1.0.0</ec-service-order-api.version>
        <ec-service-order.version>1.0.0</ec-service-order.version>
